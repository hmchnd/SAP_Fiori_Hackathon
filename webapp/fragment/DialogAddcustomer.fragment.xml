<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core">
		<Dialog title="Enter New Customer Detail" id="idDialogAddfrag" resizable="true" contentHeight="70%" contentWidth="23%">
		<content>
			<l:VerticalLayout class="sapUiContentPadding" width="100%">
				<l:content>
					<l:Grid defaultSpan="L6 M4 S6" class="sapUiSmallMarginTop sapUiSizeCompact" >
						<Label text="Customer ID" class="sapUiTinyMarginTop" required="true">
							<layoutData>
								<l:GridData span="L6 M6 S12"/>
							</layoutData>
						</Label>
						<Input id="CustomerID" />
						
					</l:Grid>
					
					
						<l:Grid defaultSpan="L6 M4 S6" class="sapUiSmallMarginTop sapUiSizeCompact" >
						<Label text="Company Name" class="sapUiTinyMarginTop">
							<layoutData>
								<l:GridData span="L6 M6 S12"/>
							</layoutData>
						</Label>
						<Input  id="CompanyName"  />
						
					</l:Grid>
					
						<l:Grid defaultSpan="L6 M4 S6" class="sapUiSmallMarginTop sapUiSizeCompact" >
						<Label text="Contact Name" class="sapUiTinyMarginTop">
							<layoutData>
								<l:GridData span="L6 M6 S12"/>
							</layoutData>
						</Label>
						<Input  id="ContactName"  />
						
					</l:Grid>
					
					
						<l:Grid defaultSpan="L6 M4 S6" class="sapUiSmallMarginTop sapUiSizeCompact" >
						<Label text="Contact Title" class="sapUiTinyMarginTop">
							<layoutData>
								<l:GridData span="L6 M6 S12"/>
							</layoutData>
						</Label>
						<Input  id="ContactTitle"  />
						
					</l:Grid>
					
						<l:Grid defaultSpan="L6 M4 S6" class="sapUiSmallMarginTop sapUiSizeCompact" >
						<Label text="Address" class="sapUiTinyMarginTop">
							<layoutData>
								<l:GridData span="L6 M6 S12"/>
							</layoutData>
						</Label>
						<Input  id="Address"  />
						
					</l:Grid>
					
					
						<l:Grid defaultSpan="L6 M4 S6" class="sapUiSmallMarginTop sapUiSizeCompact" >
						<Label text="Country" class="sapUiTinyMarginTop">
							<layoutData>
								<l:GridData span="L6 M6 S12"/>
							</layoutData>
						</Label>
						<Input  id="Country"  />
						
					</l:Grid>
					
					
						<l:Grid defaultSpan="L6 M4 S6" class="sapUiSmallMarginTop sapUiSizeCompact" >
						<Label text="City" class="sapUiTinyMarginTop">
							<layoutData>
								<l:GridData span="L6 M6 S12"/>
							</layoutData>
						</Label>
						<Input  id="City"  />
						
					</l:Grid>
					
					
						<l:Grid defaultSpan="L6 M4 S6" class="sapUiSmallMarginTop sapUiSizeCompact" >
						<Label text="Phone" class="sapUiTinyMarginTop">
							<layoutData>
								<l:GridData span="L6 M6 S12"/>
							</layoutData>
						</Label>
						<Input   id="Phone" />
						
					</l:Grid>
					
				</l:content>
			</l:VerticalLayout>
		</content>
		<beginButton>
			<Button text="Add" press="onDialogAdd"/>
		</beginButton>
		<endButton>
			<Button text="Cancel" press="onDialogAddClose"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>